{
    "version" : 2,
    "name" : "kwiksupa",
    "builds" : [
        {
            "src" : "./index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/"
        },
        { "src": "/api/(.*)", "dest": "index.js" }
    ]
}     